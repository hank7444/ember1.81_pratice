
<div class="col-xs-12 fade-block" {{bind-attr class="show:hide-opacity:show-opacity"}}>

  <div class="row">
    <div class="col-sm-12">
        <div class="page-header">

          <h1 class="pull-left">
            <a {{action back}} class="back">
              <i class="icon-mail-reply" ></i>
            </a>
            <span>營業人資料</span>
          </h1>

          <div class="pull-right">
            <ul class="breadcrumb">

            <li>
              {{#link-to "main"}}
                <i class="icon-home"></i>
              {{/link-to}}
            </li>

            <li class="separator">
              <i class="icon-angle-right"></i>
            </li>

            <li>營業人管理</li>

            <li class="separator">
              <i class="icon-angle-right"></i>
            </li>

            <li class="">營業人資料列表</li>

            <li class="separator">
              <i class="icon-angle-right"></i>
            </li>

            <li class="active">營業人資料內容</li>

            </ul>
          </div>
      </div>
    </div>
  </div>

  <div class="row">
    <div class='col-sm-12'>

  			<div class='box'>
             <div class='box bordered-box' style='margin-bottom:0;'>
              <form class='form' style='margin-bottom: 0;'>
                <fieldset>
                  <div class='col-sm-4'>
                    <div class='lead'>
                      <i class='icon-github text-contrast'></i>
                      {{!--
                      {{fullName}}
                      --}}
                    </div>
                    <small class='text-muted' id="title">大帥哥1234</small>
                  </div>
                  <div class='col-sm-7 col-sm-offset-1'>

                    <div class='form-group'>
                      <label>公司名稱</label>
                      {{companyName}}
                    </div>

                    <div class='form-group'>
                      <label>統一編號</label>
                      {{registrationNo}}
                    </div>

                    <hr class='hr-normal'>
      
                  </div>
                </fieldset>
                <hr class='hr-normal'>
              </form>
            </div>
        </div>

    </div>
  </div>

</div>